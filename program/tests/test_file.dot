digraph foo {
    graph [label="Name of the graph", fontname = "Helvetica-Oblique",
		fontsize = 36];
    n1 [color=aquamarine, b=3, a=4, style=filled, shape=polygon];
    n2 ;
    n3 [color=green, label="n3", a=3];
